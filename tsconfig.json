{
	"compilerOptions": {
		"module": "es2015",
		"target": "es5",
		"lib": ["es2018", "dom"],
		"sourceMap": true,
		"jsx": "react",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"preserveSymlinks": true,
		"moduleResolution": "node"
	},
	"exclude": ["dist", "jest", "test"]
}
